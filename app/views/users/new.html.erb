<div class="user_form">
	<% @user.errors.full_messages.each do |msg| %>
	<li><%= msg %></li>
	<% end %>
	

  <%= form_for @user do |f| %>
	<%= render partial: "form", locals: {f: f} %>
	<%= f.label :password %><br>
	<%= f.password_field :password %><br>
	<%= f.label :password_confirmation %><br>
	<%= f.password_field :password_confirmation %><br>
	<br><%= f.submit "Join GifMe" %><br>
	<% end %>
</div>